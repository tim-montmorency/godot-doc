{
  "project_name": "Godot Examples Docsh",
  "project_url": "https://gllmar.github.io/godot-examples-docsh/",
  "godot_version": "4.4.1",
  "python_version": "3.11",
  "max_parallel_jobs": null,
  "enable_web_exports": true,
  "enable_documentation_generation": true,
  "enable_embed_injection": true,
  "enable_sidebar_generation": false,
  "enable_caching": true,
  "verbose_output": false,
  "dry_run_mode": false,
  "structure": {
    "projects_dir": ".",
    "docs_dir": "docs",
    "cache_dir": ".build_cache",
    "site_root": ".",
    "sidebar_file": "_sidebar.md",
    "index_file": "index.html",
    "exports_subdir": "exports/web"
  },
  "logging": {
    "verbose_downloads": false,
    "progress_updates": true,
    "ci_mode": false
  },
  "deployment": {
    "allow_partial_failures": true,
    "max_failure_rate": 10.0,
    "exclude_godot_binaries": true,
    "exclude_templates": true
  },
  "project_include_patterns": [
    "godot-demo-projects/**/project.godot",
    "godot-demo-extended/**/project.godot"
  ],
  "project_exclude_patterns": [
    "**/.*",
    "**/addons/godot-git-plugin/**",
    "**/exports/**",
    ".github/**",
    "docs/**",
    "godot-ci-build-system/**"
  ]
}